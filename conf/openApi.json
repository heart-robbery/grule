    "/test/error": {
      "get": {
        "tags": ["test"],
        "summary": "测试抛出错误",
        "description": "抛出错误 接口",
        "parameters": [

        ],
        "responses": {
          "200": {
            "description": "成功返回",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
