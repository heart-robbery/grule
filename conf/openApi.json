  "components": {
    "schemas": {
      "ApiResp": {
        "required": ["code"],
        "type": "object",
        "properties": {
          "code": {
            "type": "string",
            "example": "00"
          },
          "desc": {
            "type": "string",
            "example": "成功"
          },
          "data": {
            "type": "object"
          },
          "traceNo": {
            "type": "string",
            "format": "uuid",
            "example": "2d59326c4ad844a6b3c866fba159a44a"
          }
        }
      }
    }
  }
